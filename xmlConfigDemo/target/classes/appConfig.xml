<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean id="e1" class="com.example.xml_config.bean.Employee" scope="prototype">
    	<property name="id" value="1"/>
    	<property name="name" value="Alice"/>
    	<property name="age" value="20"/>
    	<property name="log" ref="l1"/>
    	<property name="contact_no">
    		<list>
    			<value>1234567891</value>
    			<value>1234567892</value>
    			<value>1234567893</value>
    		</list>
    	</property>
    </bean>
    
    <bean id="l1" class="com.example.xml_config.bean.Login">
    	<property name="email" value="alice@example.com"/>
    	<property name="userName" value="Alice123"/>
    	<property name="password" value="abc@1234"/>
    </bean>
    
</beans>